<application>
  <component name="CodeInsightSettings">
    <option name="AUTOCOMPLETE_ON_CODE_COMPLETION" value="false" />
    <option name="INSERT_BRACE_ON_ENTER" value="false" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_FAMILY" value="Fira Code" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="SqlCodeCompletionSettings" USE_ALIAS_JOIN_COMPLETION="false">
    <option name="useAliasInJoinCompletion" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>