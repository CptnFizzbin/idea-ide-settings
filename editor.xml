<application>
  <component name="CodeInsightSettings">
    <option name="AUTOCOMPLETE_ON_CODE_COMPLETION" value="false" />
    <option name="INSERT_BRACE_ON_ENTER" value="false" />
    <option name="REFORMAT_ON_PASTE" value="1" />
    <option name="INDENT_TO_CARET_ON_PASTE" value="true" />
    <option name="HIGHLIGHT_SCOPE" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc; *.log" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="REMOVE_TRAILING_BLANK_LINES" value="true" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
    <option name="IS_DND_ENABLED" value="false" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="InlayHintsSettings">
    <option name="disabledHintProviderIds">
      <set>
        <option value="TypeScript.ts.type.hints" />
      </set>
    </option>
    <option name="disabledLanguages">
      <set>
        <option value="Angular2Html" />
        <option value="Groovy" />
        <option value="kotlin" />
      </set>
    </option>
    <option name="lastViewedProviderKeyId" value="ts.type.hints" />
    <settingsMapElement>
      <TypeScript.ts.type.hints>
        <Settings>
          <option name="showForFunctions" value="true" />
          <option name="showForInferredParameters" value="false" />
        </Settings>
      </TypeScript.ts.type.hints>
    </settingsMapElement>
  </component>
  <component name="SqlCodeCompletionSettings" USE_ALIAS_JOIN_COMPLETION="false" SUGGEST_ALIAS_NAMES="false">
    <option name="suggestAliasNames" value="false" />
    <option name="useAliasInJoinCompletion" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>